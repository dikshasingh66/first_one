g=("my trial")# i love my country
m=0
list=[]
trial=0
for i in range(len(g)):
    count=0
    m=i+1
    for k in list:
        if k==g[i]:
            break
        else:
            pass
    for j in range(m,len(g)):
        if g[i]!=g[j]:
            continue
        if g[i]==g[j]:
            count+=1
    if count==1:
        trial+=1
        print(g[i])
    if count>1:
        list.append(g[i])
if trial==0:
    print("None of the characters are repeating")
